{
  "name": "jedlik-typescript-vscode-template",
  "version": "1.0.0",
  "description": "Jedlik project template for using TypeScript and Node.js in VS Code.",
  "engines": {
    "node": "10.x"
  },
  "main": "app.js",
  "repository": {
    "private": true,
    "type": "git",
    "url": "https://github.com/nitslaszlo/JedlikTsTemplate"
  },
  "scripts": {
    "start": "node ./js/app.js",
    "start-debug": "node --debug=5858 ./js/app.js",
    "install:nodemodules": "npm install",
    "compile": "tsc",
    "nodemon": "nodemon --inspect -q ./js/app.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "kill": "taskkill /im node.exe /F"
  },
  "author": "Nits László",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.12.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  }
}
